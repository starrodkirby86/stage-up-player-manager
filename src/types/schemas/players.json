{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "name": {
        "type": "string"
      },
      "avatar": {
        "type": "string",
        "format": "uri"
      },
      "seed": {
        "type": "object",
        "properties": {
          "seedNum": {
            "type": "integer"
          },
          "phase": {
            "type": "string"
          }
        },
        "required": [
          "seedNum",
          "phase"
        ]
      }
    },
    "required": [
      "id",
      "name",
      "seed"
    ]
  }
}